@model IEnumerable<Vidly.Models.Movie>

@{
    
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<h2>Movies</h2>

<table class="table table-condensed table-hover">
    <thead>
    <tr>
        <th>Movies</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var movie in Model)
    {
        <tr>
            <td>@movie.Name</td>
        </tr>
    }
    </tbody>
</table>